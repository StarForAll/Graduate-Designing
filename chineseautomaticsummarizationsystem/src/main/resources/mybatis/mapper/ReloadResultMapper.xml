<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.ncu.dao.mapper.ReloadResultMapper">

    <resultMap id="reloadResultEntity" type="cn.edu.ncu.dao.entity.ReloadResult"></resultMap>

    <select id="query" resultMap="reloadResultEntity">
        SELECT * FROM reload_result
        WHERE tag = #{tag}
        ORDER BY create_time DESC
    </select>
</mapper>